{
  "name": "seeader",
  "version": "1.5.0",
  "author": "wangenius",
  "homepage": ".",
  "main": "end/main.js",
  "description": "txt book reader",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangenius/seeader.git"
  },
  "keywords": [
    "Electron",
    "txt",
    "reader"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "postinstall": "electron-builder install-app-deps",
    "package": "tsc -p electron && electron-builder",
    "devStart": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "buildAndPackage": "npm run build && tsc -p electron && electron-builder",
    "testBuild": "npm run build && tsc -p electron && electron-builder --dir"
  },
  "dependencies": {
    "electron-reload": "^2.0.0-alpha.1",
    "a_root": "file:.yalc/a_root",
    "clsx": "^1.2.1",
    "@react-spring/web": "^9.6.1",
    "@uiball/loaders": "^1.2.6",
    "epub": "^1.2.1",
    "@use-gesture/react": "^10.2.23",
    "@reduxjs/toolkit": "^1.9.1",
    "buffer": "^6.0.3",
    "i18next": "^22.4.8",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "nedb-promises": "^6.2.1",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.2",
    "react-i18next": "^12.1.4",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.4",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.4.2",
    "react-spring": "^9.5.5",
    "react-toastify": "^9.1.1",
    "react-use": "^17.4.0",
    "react-scripts": "^5.0.1",
    "js-mdict": "^4.0.20",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "history": "^5.3.0",
    "@electron/remote": "^2.0.9",
    "json-loader": "^0.5.7",
    "react-app-rewired": "^2.2.1",
    "svgo-loader": "^4.0.0",
    "svg-sprite-loader": "^6.0.11",
    "url-loader": "^4.1.1",
    "assert": "^2.0.0",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "typescript": "^4.9.3",
    "@types/node": "^18.11.18",
    "@types/webpack-env": "^1.18.0",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "wait-on": "^6.0.1"
  },
  "build": {
    "productName": "seeader",
    "appId": "com.wangenius.seeader",
    "extends": null,
    "files": [
      "build/**/*",
      "end/**/*",
      "end/*",
      "assets/*",
      "assets/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "icon": "./build/icon/icon.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icon/icon.ico",
      "createDesktopShortcut": true,
      "shortcutName": "seeader"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
